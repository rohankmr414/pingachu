{
  "name": "pingachu",
  "description": "Telegram bot for managing group roles using Cloudflare Workers and D1.",
  "private": true,
  "devDependencies": {
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "wrangler": "^4.34.0"
  },
  "scripts": {
    "cf-typegen": "wrangler types",
    "check": "tsc && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "pnpm seedLocalD1 && wrangler dev",
    "predeploy": "wrangler d1 migrations apply pingachu --remote",
    "seedLocalD1": "wrangler d1 migrations apply pingachu --local"
  },
  "dependencies": {
    "@types/node": "^24.3.1"
  }
}
